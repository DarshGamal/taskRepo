<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Test SignInUp ">
    <test name="SignIn">
        <classes>
            <class name="SignInTest">
                <methods>
                    <include name="SignIn">
                    </include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="searchProducts">
        <classes>
            <class name="SearchProductsTest">
                <methods>
                    <include name="searchProducts">
                    </include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="AddToCart">
        <classes>
            <class name="AddToCartTest">
                <methods>
                    <include name="addProduct1ToCart">
                    </include>
                    <include name="addProduct2ToCart">
                    </include>

                </methods>
            </class>
        </classes>
    </test>
    <test name="CheckOut">
        <classes>
            <class name="CheckOutTest">
                <methods>
                    <include name="ProceedToCheckout">
                    </include>
                    <include name="fillPaymentInfo">
                    </include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Contactus">
        <classes>
            <class name="ContactusTest">
                <methods>
                    <include name="fillContactusInfo">
                    </include>
                    <include name="VerifySuccessConfirmationMessage">
                    </include>
                </methods>
            </class>
        </classes>
    </test>
</suite>

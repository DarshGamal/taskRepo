<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Test SignInUp ">
    <test name="SignInTests">
        <classes>
            <class name="SignInTest">
                <methods>
                    <include name="SignIn">
                    </include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="searchProductsTests">
        <classes>
            <class name="SearchProductsTest">
                <methods>
                    <include name="searchProducts"/>
                    <include name="verifyProductDetails"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="AddToCartTests">
        <classes>
            <class name="AddToCartTest">
                <methods>
                    <include name="addProduct1ToCart"/>
                    <include name="continueShopping"/>
                    <include name="addProduct2ToCart"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="CheckOut">
        <classes>
            <class name="CheckOutTest">
                <methods>
                    <include name="ProceedToCheckout"/>
                    <include name="fillPaymentInfo"/>
                    <include name="submitOrderPayment"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Contactus">
        <classes>
            <class name="ContactusTest">
                <methods>
                    <include name="fillContactusInfo"/>
                    <include name="VerifySuccessConfirmationMessage"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Logout">
        <classes>
            <class name="LogoutTest">
                <methods>
                    <include name="clickOnLogoutBtn"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
